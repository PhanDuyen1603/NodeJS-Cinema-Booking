<% const filmXemNhieu = filmChieu.filmXemNhieu %> 
<% for (var i = 0; i < filmXemNhieu.length; i++ ) { %>
        <div class="col-md-3 col-sm-3 movie-item">
            <div class="article-movie-home">
                <a href="/phim/<%= filmXemNhieu[i].dataValues.film_ID %>">
                    <img id="img-film-indexContent" src="<%= filmXemNhieu[i].dataValues.film_Image %>" alt="Không load được ảnh" width="216" height="324">
                </a>
            </div>
            <a href="/phim/<%= filmXemNhieu[i].dataValues.film_ID %>">
                <div class="title-movie">
                    <h4><%= filmXemNhieu[i].dataValues.film_Name %></h4>
                </div>
            </a>
        </div>
<% } %>
