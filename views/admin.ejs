        </ul>
    </div>
    <div id="content_admin">
    <% if(typeof thuocTinhFilm !== 'undefined' && typeof thuocTinhCumRap !== 'undefined') { %>
        <% include admin/BangDoanhThu.ejs %>
    <% } %>
    <% if(typeof cinema !== 'undefined') { %>
        <% include admin/cinema.ejs %>
    <% } %>
    <% if(typeof cinemaID !== 'undefined') { %>
        <% include admin/update/updatecinema.ejs %>
    <% } %>
    <% if(typeof userAll !== 'undefined') { %>
        <% include admin/userAll.ejs %>
    <% } %>
    <% if(typeof userID !== 'undefined') { %>
        <% include admin/update/updateuser.ejs %>
    <% } %>
    <% if(typeof filmAll !== 'undefined') { %>
        <% include admin/filmAll.ejs %>
    <% } %>
    <% if(typeof filmID !== 'undefined') { %>
        <% include admin/update/updatefilm.ejs %>
    <% } %>
    <% if(typeof cineplex !== 'undefined') { %>
        <% include admin/cineplex.ejs %>
    <% } %>
    <% if(typeof timeShowCinemaIDcinema !== 'undefined' ) { %>
        <% include admin/cinemaTimeShow/cinemaTimeShowCinema.ejs %>
    <% } %>
