<!DOCTYPE html>
<html>

<head>
    <title>Admin web đặt vé phim</title>
    <script src="/public/js/yesOrNo_Admin.js"></script>
    <script src="/public/js/jquery/jquery-3.4.1.min.js"></script>
    <script src="/public/js/jquery/adminjq.js"></script>
    <link href="/public/css/admin/adminhome.css" rel="stylesheet" />
    <link href="/public/css/admin/tableAdmin.css" rel="stylesheet" />
    <link href="/public/css/admin/all.css" rel="stylesheet" />
    <link href="/public/css/admin/w3.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />

    <!-- MỚI THÊM FONT AWESOME 04/06/2021 -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"
        integrity="sha512-iBBXm8fW90+nuLcSKlbmrPcLa0OT92xO1BIsZ+ywDWZCvqsWgccV3gFoRBv0z+8dLJgyAHIhR35VZc2oM/gI1w=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>

<body>
    <div class="d-flex">
        <div class="d-flex flex-column flex-shrink-0 p-3 text-white bg-dark" style="width: 265px">
            <a href="/" class="
            d-flex
            align-items-center
            mb-3 mb-md-0
            me-md-auto
            text-white text-decoration-none
          ">
                <div class="logo logo--medium">
                    <a href="/ " class="router-link-active"><img src="/public/image/Logo-Dien-03.png "
                            alt="Một Chút Phim Accounts " class="logo-image"
                            style="width: 150px; margin-top: 30px;" /></a>
                </div>
            </a>
            <ul class="nav nav-pills flex-column mb-auto navbar-nav">
                <li>
                    <i class="fas fa-user-shield me-2"></i>
                    <a href="/admin" id="logo" class="active">Admin</a>
                </li>
                <li>
                    <i class="far fa-calendar-minus mr-2"></i>
                    <a href="/admin/showtime">Quản lý xuất chiếu</a>
                </li>
                <li>
                    <i class="fas fa-film mr-2"></i>
                    <a href="/admin/film">Quản lý phim</a>
                </li>
                <li>
                    <i class="fas fa-door-open"></i>
                    <a href="/admin/cinema">Quản lý rạp</a>
                </li>
                <li>
                    <i class="fas fa-hotel"></i>
                    <a href="/admin/cineplex">Quản lý cụm rạp</a>
                </li>
                <li>
                    <i class="fas fa-user me-2"></i>
                    <a href="/admin/user">Quản lý người dùng</a>
                </li>
                <li>
                    <i class="fas fa-sign-out-alt"></i>
                    <a href="/admin/logout">Sign out</a>
                </li>
            </ul>
        </div>

        <div style="width: 1222px; float: left; right: 20px; margin-left: 280px;">
            <% if(typeof filmStatistic !=='undefined' && typeof cineplexStatistic !=='undefined' ) { %>
                <% include admin/stats %>
                    <% } %>
                        <% if(typeof showtime !=='undefined' ) { %>
                            <% include admin/showtime %>
                                <% } %>
                                    <% if(typeof showtimesByFilm !=='undefined' ) { %>
                                        <% include admin/showtimesByFilm %>
                                            <% } %>
                                                <% if(typeof showtimesByCinema !=='undefined' ) { %>
                                                    <% include admin/showtimesByCinema %>
                                                        <% } %>
                                                            <% if(typeof cinema !=='undefined' ) { %>
                                                                <% include admin/cinema.ejs %>
                                                                    <% } %>
                                                                        <% if(typeof userAll !=='undefined' ) { %>
                                                                            <% include admin/userAll.ejs %>
                                                                                <% } %>
                                                                                    <% if(typeof filmAll !=='undefined'
                                                                                        ) { %>
                                                                                        <% include admin/filmAll.ejs %>
                                                                                            <% } %>
                                                                                                <% if(typeof cineplex
                                                                                                    !=='undefined' ) {
                                                                                                    %>
                                                                                                    <% include
                                                                                                        admin/cineplex.ejs
                                                                                                        %>
                                                                                                        <% } %>
        </div>
    </div>
    <script>
    </script>
